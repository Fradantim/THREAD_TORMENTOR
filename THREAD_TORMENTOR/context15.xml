<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	

	
	<bean id="NEGOCIO" class="java.lang.String">
    	<constructor-arg value="movil" />
	</bean>
	
	<bean id="LOTE_ID" class="java.lang.String">
    	<constructor-arg value="ML" />
	</bean>
	
	<bean id="Job_1" class="model.Doc1gen">
		<property name="hip" value="$DOC1_GEN_SRC$/AMDOCS_T3_CICLICA.hip" />
		<property name="opsFile" value="$DOC1_GEN_SRC$/config_doc1gen_N.ops" />
		<property name="him">
			<list>
	        	 <value>$DOC1_GEN_HIM$/AMD_$CICLO$_$PERIODO$.him</value>
	        	 <value>$DOC1_GEN_HIM$/AMD_NN_$PERIODO$.him</value>
	        </list>
        </property>
		<property name="next">
			<list>
	        	 <ref bean ="Job_FIN"/>
	        </list>
        </property>
	</bean>

	<bean id="Job_FIN" class="model.Finisher">
		<property name="next"> 
			<list/>
		</property>
	</bean>
</beans>